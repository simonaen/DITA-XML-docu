                                        </fo:list-block>
                                    </fo:block>
                                    <fo:block-container space-after="1em" space-before="1em" id="unique_8_Connect_42_result_bzh_ggp_3pb" chfox:nd-id="result_bzh_ggp_3pb"><fo:block end-indent="0" start-indent="0" role="Sect">
                                        <fo:block-container space-after="0.5em" margin-left="0" margin-right="0" space-before="0.5em"><fo:block end-indent="0" start-indent="0">The unwanted
                                            email is moved to the spam folder. Future emails from
                                            this sender will be moved there as well.</fo:block></fo:block-container>
                                    </fo:block></fo:block-container>
                                </fo:block></fo:block-container>
                            </fo:block>
